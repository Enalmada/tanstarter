		"autoprefixer": "^10.4.21",
		"babel-plugin-react-compiler": "^19.1.0-rc.3",
		"cross-env": "^10.1.0",
		"drizzle-kit": "^0.31.6",
		"globals": "^16.5.0",
		"happy-dom": "^20.0.10",
		"lefthook": "^2.0.2",
		"npm-check-updates": "^19.1.2",
		"serwist": "^9.2.1",
		"storybook": "^10.0.2",
		"tailwindcss": "^4.1.16",
		"turbo": "^2.6.0",
		"typescript": "^5.9.3",
