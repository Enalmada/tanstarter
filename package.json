		"vitest": "^4.0.6"
	},
	"description": "production ready boilerplate for Tanstack Starter",
	"private": true,
	"main": "",
	"packageManager": "bun@1.3.1",
	"trustedDependencies": [
		"@biomejs/biome",
		"@parcel/watcher",
		"@swc/core",
		"@tailwindcss/oxide",
		"bufferutil",
		"core-js",
		"esbuild",
		"lefthook",
		"sharp"
	],
	"type": "module"
}
