{
	"name": "tanstarter",
	"version": "0.1.0",
	"scripts": {
		"build": "vinxi build",
		"build:prod": "bun run build && bun run drizzle:migrate && bun run scripts/post-build.ts",
		"check-types": "tsc --noEmit",
		"compile": "lingui compile",
		"db": "drizzle-kit",
		"dev": "bun run docker:up && turbo dev:vinxi --log-prefix=none",
		"dev:install": "bun install",
		"dev:vinxi": "bun app/validate-env.ts && vinxi dev",
		"docker:up": "docker compose --env-file .env up -d --remove-orphans",
		"drizzle:generate": "dotenv -e ./.env drizzle-kit generate",
		"drizzle:migrate": "bun ./app/server/db/migrate.ts",
		"extract": "lingui extract",
		"fix": "fixpack",
		"lint": "biome check --fix --unsafe",
		"prepare": "lefthook install",
		"start": "vinxi start",
		"storybook": "cross-env NODE_OPTIONS=--no-warnings storybook dev -p 6006",
		"build-storybook": "cross-env NODE_OPTIONS=--no-warnings storybook build",
		"test-storybook": "cross-env NODE_OPTIONS=--no-warnings test-storybook",
		"ui": "pnpm dlx shadcn@canary",
		"test:unit": "cross-env NODE_ENV=test dotenv -o -e .env.test.local -e .env.test -- bunx vitest run",
		"test:unit:coverage": "cross-env NODE_ENV=test dotenv -e .env.test.local -e .env.test -- vitest --coverage",
		"test:unit:watch": "cross-env NODE_ENV=test dotenv -o -e .env.test.local -e .env.test -- vitest watch",
		"test:e2e": "cross-env CI=false playwright test",
		"test:e2e:member": "playwright test --project=member",
		"test:e2e:admin": "playwright test --project=admin",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:codegen": "playwright codegen http://localhost:3000",
		"test:e2e:report": "bunx playwright show-report playwright/report --host 127.0.0.1 --port 9323"
	},
	"dependencies": {
		"@axiomhq/js": "^1.3.1",
		"@casl/ability": "^6.7.3",
		"@enalmada/drizzle-helpers": "0.0.18",
		"@hookform/resolvers": "^4.1.3",
		"@lingui/babel-plugin-lingui-macro": "^5.2.0",
		"@lingui/cli": "^5.2.0",
		"@lingui/macro": "^5.2.0",
		"@lingui/react": "^5.2.0",
		"@lukemorales/query-key-factory": "^1.3.4",
