{
	"name": "tanstarter",
	"version": "0.1.0",
	"scripts": {
		"build": "vinxi build",
		"build:prod": "bun run build && bun run drizzle:migrate",
		"check-types": "tsc --noEmit",
		"db": "drizzle-kit",
		"dev": "turbo dev:vinxi --log-prefix=none",
		"dev:install": "bun install",
		"dev:vinxi": "bun app/validate-env.ts && vinxi dev",
		"docker:up": "docker-compose --env-file .env up -d --remove-orphans",
		"drizzle:generate": "dotenv -e ./.env drizzle-kit generate",
		"drizzle:migrate": "bun ./app/server/db/migrate.ts",
		"lint": "biome check --fix --unsafe",
		"pre-commit": "bun lint-staged",
		"prepare": "husky",
		"start": "vinxi start",
		"ui": "pnpm dlx shadcn@latest"
	},
	"dependencies": {
		"@enalmada/drizzle-helpers": "0.0.18",
		"@neondatabase/serverless": "^0.10.4",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@radix-ui/react-slot": "^1.1.1",
		"@tanstack/react-query": "^5.62.15",
		"@tanstack/react-query-devtools": "^5.62.15",
		"@tanstack/react-router": "^1.95.1",
		"@tanstack/react-router-with-query": "^1.95.1",
		"@tanstack/router-devtools": "^1.95.1",
		"@tanstack/start": "^1.95.1",
		"@vercel/postgres": "^0.10.0",
		"@vitejs/plugin-react": "^4.3.4",
		"arctic": "^3.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.38.3",
		"lucide-react": "^0.469.0",
		"nanoid": "^5.0.9",
		"postgres": "^3.4.5",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwind-merge": "^2.6.0",
		"tailwindcss-animate": "^1.0.7",
		"unenv": "^1.10.0",
		"vinxi": "^0.5.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@enalmada/clone-code": "0.1.1",
		"@serwist/vite": "^9.0.11",
		"@serwist/window": "^9.0.11",
		"@types/react": "^19.0.2",
		"@types/react-dom": "^19.0.2",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.30.1",
		"fixpack": "^4.0.0",
		"globals": "^15.14.0",
		"husky": "9.1.7",
		"lint-staged": "15.3.0",
		"postcss": "^8.4.49",
		"serwist": "^9.0.11",
		"tailwindcss": "^3.4.17",
		"turbo": "^2.3.3",
		"typescript": "^5.7.2",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"description": "production ready boilerplate for Tanstack Starter",
	"private": true,
	"main": "",
	"packageManager": "bun@1.1.42",
	"trustedDependencies": [
		"@biomejs/biome",
		"@parcel/watcher",
		"bufferutil",
		"esbuild"
	],
	"type": "module"
}
